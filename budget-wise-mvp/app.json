      },
      "package": "com.everton_prdo.budgetwisemvp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "851eac4a-a5db-48a5-ab3a-ca7d75d948b0"
